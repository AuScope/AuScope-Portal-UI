{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false,
    "schematicCollections": ["@angular-eslint/schematics"]
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "project": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "preserveSymlinks": true,
            "aot": true,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "outputHashing": "all",
            "sourceMap": false,
            "namedChunks": false,
            "extractLicenses": true,
            "vendorChunk": false,
            "optimization": {
              "scripts": false,
              "styles": {
                "minify": true,
                "inlineCritical": true
              },
              "fonts": true
            },
            "buildOptimizer": false,
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/contexts",
              "src/template",
              "src/extension",
              "src/favicon-16x16.png",
              "src/favicon-32x32.png",
              {
                "glob": "**/*",
                "input": "./node_modules/cesium/Build/Cesium",
                "output": "./assets/cesium"
              }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/purple-green.css",
              "src/styles.scss",
              "node_modules/rickshaw/rickshaw.min.css",
              "src/extension/css/extensions.css",
              "node_modules/nouislider/distribute/nouislider.min.css",
              "src/extension/css/d3.css",
              "node_modules/@ng-select/ng-select/themes/default.theme.css",
              "./node_modules/cesium/Build/Cesium/Widgets/widgets.css"
            ],
            "scripts": [
              "node_modules/d3/d3.min.js",
              "node_modules/rickshaw/rickshaw.min.js",
              "src/extension/js/extensions.js",
              "src/extension/js/Rickshaw.Graph.RangeSlider.MultiPreview.min.js",
              "src/extension/js/Rickshaw.Graph.SmootherPlus.min.js",
              "node_modules/lzma/src/lzma_worker.js",
              "src/extension/js/box.js",
              "src/extension/js/d3.legend.js",
              "src/extension/js/no-ie-support.js",
              "./node_modules/cesium/Build/Cesium/Cesium.js"
            ],
            "customWebpackConfig": {
              "path": "./extra-webpack.config.js"
            }
          },
          "configurations": {
            "audev": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ]
            },
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "project:build",
            "proxyConfig": "./proxy.conf.json"
          },
          "configurations": {
            "audev": {
              "browserTarget": "project:build:audev"
            },
            "production": {
              "browserTarget": "project:build:production"
            },
            "development": {
              "browserTarget": "project:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "project:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/d3/d3.min.js",
              "node_modules/rickshaw/rickshaw.min.js",
              "src/extension/js/extensions.js",
              "src/extension/js/Rickshaw.Graph.RangeSlider.MultiPreview.min.js",
              "src/extension/js/Rickshaw.Graph.SmootherPlus.min.js",
              "node_modules/lzma/src/lzma_worker-min.js",
              "src/extension/js/box.js",
              "src/extension/js/d3.legend.js"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.scss",
              "node_modules/rickshaw/rickshaw.min.css",
              "src/extension/css/extensions.css",
              "node_modules/nouislider/distribute/nouislider.min.css",
              "src/extension/css/d3.css"
            ],
            "assets": [
              "src/template",
              "src/extension",
              "src/favicon-16x16.png",
              "src/favicon-32x32.png"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        }
      }
    },
    "project-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "project:serve"
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
