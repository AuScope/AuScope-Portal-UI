{
  "routes": [
    {
      "route": "/geonetwork/*",
      "rewrite": "https://augeonetwork-dev.geoanalytics.group/geonetwork/"
    },
    {
      "route": "/login/*",
      "methods": ["GET","HEAD","POST","OPTIONS"],
      "rewrite": "index.html"
    }
  ],  
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
